local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Phantom gui", "Midnight")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Back Front Flip", "Idk", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/7wDcPtLk'))()
end)

MainSection:NewToggle("Super-Human", "Superman ðŸ¤«", function(state)
    if state then
        game.Players.LocalPlayers.Character.Humanoid.WalkSpeed = 120
        game.Players.LocalPlayers.Character.Humanoid.JumpPower = 120
    else
       game.Players.LocalPlayers.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayers.Character.Humanoid.JumpPower = 50
    end
end)
