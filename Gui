local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Phantom gui", "Midnight")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Infinite jump", "gives you infinite jump", function()
    pcall(loadstring(game:HttpGet("https://pastebin.com/raw/2wgbZ6Xd")))
end)

MainSection:NewToggle("Super-Human", "Superman ðŸ¤«", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
wait (0.2)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
    else
       game.Players.LocalPlayers.Character.Humanoid.WalkSpeed = 16.8
        game.Players.LocalPlayers.Character.Humanoid.JumpPower = 52
    end
end) 
